{
  "JiwaAPIURL": "https://yourJiwaapi.com:1234",
  // JiwaAPIKey is only used to make a GET request on /Queries/ContactNameMultiples if the customer login throws an exception because multiple debtor contacts share the same email address.
  // So, therfore the key used here only requires GET permission on the route /Queries/ContactNameMultiples and nothing else
  "JiwaAPIKey": "{Your API Key configured in Jiwa}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://*:80"
      }
    }
  }
}

